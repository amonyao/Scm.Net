<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>disable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>Com.Scm.Api</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="logs\**" />
    <Compile Remove="Publish\**" />
    <Compile Remove="quartz\**" />
    <Content Remove="logs\**" />
    <Content Remove="Publish\**" />
    <Content Remove="quartz\**" />
    <EmbeddedResource Remove="logs\**" />
    <EmbeddedResource Remove="Publish\**" />
    <EmbeddedResource Remove="quartz\**" />
    <None Remove="logs\**" />
    <None Remove="Publish\**" />
    <None Remove="quartz\**" />
  </ItemGroup>

  <ItemGroup>
    <Content Remove="data\unit.json" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="data\scm.db" />
    <None Remove="data\scm.uid" />
  </ItemGroup>

  <ItemGroup>
    <None Include="data\unit.json" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Samples.Server\Samples.Server.csproj" />
    <ProjectReference Include="..\Scm.Cache\Scm.Cache.csproj" />
    <ProjectReference Include="..\Scm.Common\Scm.Common.csproj" />
    <ProjectReference Include="..\Scm.Core\Scm.Core.csproj" />
    <ProjectReference Include="..\Scm.Email\Scm.Email.csproj" />
    <ProjectReference Include="..\Scm.Generator\Scm.Generator.csproj" />
    <ProjectReference Include="..\Scm.Server.Cache\Scm.Server.Cache.csproj" />
    <ProjectReference Include="..\Scm.Server.Quartz\Scm.Server.Quartz.csproj" />
    <ProjectReference Include="..\Scm.Server.Service\Scm.Server.Service.csproj" />
    <ProjectReference Include="..\Scm.Server.SignalR\Scm.Server.SignalR.csproj" />
    <ProjectReference Include="..\Scm.Server.SqlSugar\Scm.Server.SqlSugar.csproj" />
    <ProjectReference Include="..\Scm.Server.Swagger\Scm.Server.Swagger.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="9.0.9" />
    <PackageReference Include="Serilog.Settings.Configuration" Version="9.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Scm.Uid">
      <HintPath>..\Libs\net9.0\Scm.Uid.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
