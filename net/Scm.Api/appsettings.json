{
  "AllowedHosts": "*",
  "Serilog": {
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "MinimumLevel": "Information",
    "Properties": {
      "Application": "C-SCM"
    },
    "Using": [ "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "OutputTemplate": "[{Timestamp:HH:mm:ss} {Level:u11}] {Message:lj}{NewLine}"
        }
      }
    ]
  },
  "Env": {
    "dataUri": "/data",
    "dataDir": "data"
  },
  "Sql": {
    "Type": "MySQL",
    "Text": "server=localhost;database=scm;uid=root;pwd=123456;charset=utf8mb4;SslMode=None;"
  },
  "Uid": {
