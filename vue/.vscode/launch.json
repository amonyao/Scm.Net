{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "启动程序: chrome",
      "url": "http://localhost:2800",
      "webRoot": "${workspaceFolder}/vue",
    },
    {
      "type": "msedge",
      "request": "launch",
      "name": "启动程序：Edge",
      "url": "http://localhost:2800",
      "webRoot": "${workspaceFolder}/vue"
    },
    {
      "name": "启动程序: firefox",
      "request": "launch",
      "type": "firefox",
      "url": "http://localhost:2800",
      // "firefoxExecutable": "D:\\Program Files\\Firefox\\firefox.exe",
      "webRoot": "${workspaceFolder}/vue",
      "pathMappings": [
        {
          "url": "webpack://c-scm/src/views/login",
          "path": "${workspaceFolder}/src/views/login"
        },
        {
          "url": "webpack://c-scm",
          "path": "${workspaceFolder}"
        }
      ]
    },
    {
      "name": "运行脚本：serve",
      "command": "npm run serve",
      "type": "node-terminal",
      "request": "launch",
    }
  ]
}